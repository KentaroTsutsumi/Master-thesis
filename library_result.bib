@article{Hill2010,
abstract = {Mesoporous eskolaite, Cr 2 O 3 prepared by using the evaporation method with mesoporous KIT-6 silica as a hard template has been analysed with SQUID magnetometry and variable temperature neutron powder diffraction (NPD). NPD analysis shows that the magnetic and crystallographic properties of the eskolaite structure within the walls of the mesoporous Cr 2 O 3 material are identical to that of “bulk” material, R 3 \UTF{00AF} c . The refined magnetic moments on the Cr 3 + ions are lower than observed for bulk material due to the higher proportion of surface atoms, creating local disorder. This is corroborated by the magnetometry results, which suggest a small amount of ferromagnetism may be present at low temperatures.},
author = {Hill, Adrian H and Harrison, Andrew and Dickinson, Calum and Zhou, Wuzong and Kockelmann, Winfried},
doi = {http://dx.doi.org/10.1016/j.micromeso.2009.11.021},
file = {:Users/tsujimotonaoto/Documents/論文/readcube/Hill2010.pdf:pdf},
issn = {1387-1811},
journal = {Microporous and Mesoporous Materials},
keywords = {Cr    2      O    3,Magnetism,Mesoporous,Neutron diffraction},
number = {1\UTF{2013}3},
pages = {280},
publisher = {Elsevier Inc.},
title = {{Crystallographic and magnetic studies of mesoporous eskolaite}},
url = {http://www.sciencedirect.com/science/article/pii/S1387181109005356},
volume = {130},
year = {2010}
}

@article{Ye2014,
abstract = {Magnetoelectric (ME) materials are of fundamental interest and have \begin{array}
>
\end{array}\UTF{2022}een investigated for their broad potential for technological applications. The search for, and eventually the theoretical design of, materials with large ME couplings present challenging issues. First-principles methods have only recently been developed to calculate the full ME response tensor {\$}\backslashalpha{\$} including both electronic and ionic (i.e., lattice-mediated) contributions. The latter is proportional to both the Born dynamical electric charge {\$}Z{\^{}}{\{}\backslashrm e{\}}{\$} and its analogue, the dynamical magnetic charge {\$}Z{\^{}}{\{}\backslashrm m{\}}{\$}. Here we present a theoretical study of the magnetic charge {\$}Z{\^{}}{\{}\backslashrm m{\}}{\$} and the mechanisms that could enhance it. Using first-principles density-functional methods, we calculate the atomic {\$}Z{\^{}}{\{}\backslashrm m{\}}{\$} tensors in {\$}\backslashrm{\{}Cr{\_}2O{\_}3{\}}{\$}, a prototypical magnetoelectric, and in KITPite, a fictitious material that has previously been reported to show a strong ME response arising from exchange striction effects. Our results confirm that in {\$}\backslashrm{\{}Cr{\_}2O{\_}3{\}}{\$}, the {\$}Z{\^{}}{\{}\backslashrm m{\}}{\$} values and resulting ME responses arise only from spin-orbit coupling (SOC) and are therefore rather weak. In KITPite, by contrast, the exchange striction acting on the non-collinear spin structure induces much {\$}Z{\^{}}{\{}\backslashrm m{\}}{\$} values that persist even when SOC is completely absent.},

file = {:Users/tsujimotonaoto/Documents/論文/readcube/Ye2014.pdf:pdf},
issn = {1098-0121},
journal = {Physical Review B},
number = {February},
pages = {064301},
title = {{Dynamical magnetic charges and linear magnetoelectricity}},
url = {http://arxiv.org/abs/1401.1538},
volume = {89},
year = {2014}
